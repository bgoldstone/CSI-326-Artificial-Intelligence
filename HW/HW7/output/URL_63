var variableslide=new Array()
 
//variableslide[x]=["path to image", "OPTIONAL link for image", "OPTIONAL text description (supports HTML tags)"]

variableslide[0]=['/images/main/athletics/men/soccer/msocbig_010815.jpg', 'winter15/0108.html', '<center><strong><font color=white>January 8: Dotzman adds second All-America nod</font></strong></center>']
