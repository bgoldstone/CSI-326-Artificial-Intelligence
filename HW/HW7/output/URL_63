variableslide[2]=['/images/main/athletics/women/basketball/wbbbig_010715.jpg', 'winter15/0107page2.html', '<center><strong><font color=white>January 7: Women&#8217;s basketball hands McDaniel first loss</font></strong></center>']
variableslide[3]=['/images/main/athletics/men/basketball/mbbbig_010515.jpg', 'winter15/0105.html', '<center><strong><font color=white>January 5: Men&#8217;s basketball falls in last second</font></strong></center>']
variableslide[4]=['/images/main/athletics/men/wrestling/wrebig_010315.jpg', 'winter15/0103.html', '<center><strong><font color=white>January 3: Gimbar wins on Long Island</font></strong></center>']
variableslide[5]=['/images/main/athletics/mulebig_123014.jpg', ' http://youtu.be/-K393rMqU7U', '<center><strong><font color=white>December 30: 14 for &#8217;14: Year in Review</font></strong></center>']
variableslide[6]=['/images/main/athletics/men/football/fbbig_122314.jpg', 'winter15/1223.html', '<center><strong><font color=white>December 23: Five honored by ECAC in football</font></strong></center>']
variableslide[7]=['/images/main/athletics/mulebig_122214.jpg', 'winter15/1222.html', '<center><strong><font color=white>December 22: Mules sweep basketball awards</font></strong></center>']

//configure the below 3 variables to set the dimension/background color of the slideshow
 
var slidewidth='780px' //set to width of LARGEST image in your slideshow
var slideheight='470px' //set to height of LARGEST iamge in your slideshow, plus any text description
var slidebgcolor='#000000'
 
//configure the below variable to determine the delay between image rotations (in miliseconds)
var slidedelay=4000
 
////Do not edit pass this line////////////////
 
var ie=document.all
var dom=document.getElementById
 
for (i=0;i<variableslide.length;i++){
var cacheimage=new Image()
cacheimage.src=variableslide[i][0]
}
 
var currentslide=0
 
function rotateimages(){
contentcontainer='<center>'
if (variableslide[currentslide][1]!="")
contentcontainer+='<a href="'+variableslide[currentslide][1]+'">'
contentcontainer+='<img src="'+variableslide[currentslide][0]+'" border="0" vspace="3">'
if (variableslide[currentslide][1]!="")
contentcontainer+='</a>'
contentcontainer+='</center>'
if (variableslide[currentslide][2]!="")
contentcontainer+=variableslide[currentslide][2]
 
if (document.layers){
crossrotateobj.document.write(contentcontainer)
crossrotateobj.document.close()
}
else if (ie||dom)
crossrotateobj.innerHTML=contentcontainer
if (currentslide==variableslide.length-1) currentslide=0
else currentslide++
setTimeout("rotateimages()",slidedelay)
}
 
if (ie||dom)
document.write('<div id="slidedom" style="width:'+slidewidth+';height:'+slideheight+'; background-color:'+slidebgcolor+'"></div>')
 
function start_slider(){
crossrotateobj=dom? document.getElementById("slidedom") : ie? document.all.slidedom : document.slidensmain.document.slidenssub
if (document.layers)
document.slidensmain.visibility="show"
rotateimages()
}
 
if (ie||dom)
start_slider()
else if (document.layers)
window.onload=start_slider
 
</script>
 
<ilayer id="slidensmain" width=&{slidewidth}; height=&{slideheight}; bgColor=&{slidebgcolor}; visibility=hide><layer id="slidenssub" width=&{slidewidth}; left=0 top=0></layer></ilayer>

