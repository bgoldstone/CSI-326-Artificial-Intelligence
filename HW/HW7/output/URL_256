<!-- old Js -->
<script type="text/javascript" src="/media/demofastspot/oldJs.js"></script>
<!-- Admission Counselor -->
<script>
  
(function() {
  
  var state_items = " ABEF, Academics, Accounting, Admissions, Advancement, Africana Studies, American Studies, Analytics, Art, Arts and Culture, Asian Studies, Athletics, Biochemistry, Biology, Business, Center for Ethics, Chemistry, Commencement, Community Engagement, Computer Science, Continuing Studies, Creative Writing, Dance, Data Analytics, Diversity &amp; Inclusion, Documentary Storymaking, Economics, Education, English, Environmental Science, Faculty News, Film Studies, Finance, French, German, Global Education, History, Innovation &amp; Entrepreneurship, International Studies, Italian, Jewish Studies, Latin American &amp; Caribbean Studies, Lectures &amp; Forums, LLC, Math, MathCS, Media and Communication, Music, Neuroscience, Philosophy, Physics, Political Economy &amp; Public Policy, Political Science, Powerful Outcomes, Prehealth, Prelaw, President's Office, Psychology, Public Health, Religion Studies, Religious Life, Research and Scholarship, Russian, Scholarships and Awards, Sociology &amp; Anthropology, Spanish, Staff News, Statistics, Student Life, Sustainability Studies, Theatre, Theatre and Dance, Women's &amp; Gender";


	state_items = state_items.split(',');
    state_items = state_items.sort();
    $.each(state_items, function(i, v) {
        v = v.trim();
      (v) ? $('#counselor-dropdown').append('<option value="' + v + '">' + v + '</option>') : null;
    });
    

   var url = window.location.href;
  var queryString = url ? url.split('?')[1] : window.location.search.slice(1);
if (queryString){
  var keyword = queryString.split('=');

     keyword[1] = keyword[1].split("%20").join(" ");
     keyword[1] = keyword[1].split("%27").join("'");
 
    document.getElementById("counselor-dropdown").value= keyword[1];

 
   var filter = keyword [1],
              activeItems = [];
        $.each($('[data-counselor-item]'), function(i, v) {
            if (filter) {
            	var possibilities = $(this).attr('data-counselor-locations').split(',');
                $.each(possibilities, function(i, v) {
                    v = v.trim();
                    possibilities[i] = v;
                });
                if (possibilities.indexOf(filter) > -1) {
                    $(this).fadeIn();
                    activeItems.push(v);
                } else {
                    $(this).fadeOut();
                }
            } else {
                activeItems.push(v);
                $(this).fadeIn();
            }
          });
  
  }
  
    $('#counselor-dropdown').on('change', function() {
        var filter = $(this).val(),
            activeItems = [];
        $.each($('[data-counselor-item]'), function(i, v) {
            if (filter) {
            	var possibilities = $(this).attr('data-counselor-locations').split(',');
                $.each(possibilities, function(i, v) {
                    v = v.trim();
                    possibilities[i] = v;
                });
                if (possibilities.indexOf(filter) > -1) {
                    $(this).fadeIn();
                    activeItems.push(v);
                } else {
                    $(this).fadeOut();
                }
            } else {
                activeItems.push(v);
                $(this).fadeIn();
            }
        });
        if (!activeItems.length) {
        	$('.no-counselors').fadeIn();
            $('.no-counselors .location').text(filter);
        } else {
            $('.no-counselors').fadeOut();
        }
    });

  })();
</script>


